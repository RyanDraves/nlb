{
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "bazel.buildifierExecutable": "buildifier",
    "bazel.buildifierFixOnFormat": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "python.defaultInterpreterPath": "venv/bin/python",
    "files.associations": {
        "*.ejs": "html",
        "cstddef": "cpp",
        "random": "cpp",
        "complex": "cpp",
        "cstdint": "cpp",
        "array": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "*.inc": "cpp",
        "bitset": "cpp",
        "rope": "cpp",
        "regex": "cpp",
        "any": "cpp",
        "charconv": "cpp",
        "compare": "cpp",
        "functional": "cpp",
        "ratio": "cpp",
        "scoped_allocator": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "variant": "cpp",
        "chrono": "cpp",
        "memory_resource": "cpp",
        "vector": "cpp",
        "ostream": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "system_error": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "cinttypes": "cpp",
        "typeindex": "cpp",
        "typeinfo": "cpp",
        "codecvt": "cpp",
        "condition_variable": "cpp",
        "ctime": "cpp",
        "fstream": "cpp",
        "iomanip": "cpp",
        "semaphore": "cpp",
        "stop_token": "cpp",
        "thread": "cpp",
        "valarray": "cpp",
        "format": "cpp",
        "coroutine": "cpp",
        "csignal": "cpp",
        "future": "cpp",
        "source_location": "cpp"
    },
    "files.exclude": {
        "**/__pycache__": true,
    },
    "python.analysis.exclude": [
        "venv/**",
        "bazel-*/**",
        "external/**",
    ],
    "python.analysis.typeCheckingMode": "standard",
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always",
        },
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file",
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnType": false
    },
}
