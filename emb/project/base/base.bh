message Ping {
    uint8_t ping;
}

message FlashPage {
    uint32_t address;
    uint32_t read_size;
    list[uint8_t] data;
}

message LogMessage {
    string message;
}


transaction ping[Ping, LogMessage];
transaction flash_page[FlashPage, FlashPage];
transaction read_flash_page[FlashPage, FlashPage];
